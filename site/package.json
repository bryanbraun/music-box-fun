{
  "name": "music-box-fun",
  "version": "1.0.0",
  "description": "A web app for making music box songs",
  "author": "Bryan Braun",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bryanbraun/music-box-fun.git"
  },
  "scripts": {
    "test": "cypress run",
    "test:prod": "cypress run --config baseUrl=https://musicbox.fun",
    "test:static": "cypress run --spec 'cypress/e2e/*[!.api].cy.js'",
    "test:visual": "cypress open",
    "build": "node build-modules.mjs",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "cypress": "^10.6.0",
    "esinstall": "^1.1.7"
  },
  "dependencies": {
    "@tonejs/midi": "2.0.28",
    "anchor-js": "5.0.0",
    "classnames": "2.2.6",
    "json-url": "2.4.0",
    "lory.js": "2.5.3",
    "tone": "14.7.58"
  },
  "buildModules": {
    "install": [
      "@tonejs/midi",
      "anchor-js",
      "classnames",
      "json-url",
      "lory.js",
      "tone"
    ],
    "installOptions": {
      "dest": "public/js/vendor"
    }
  }
}
